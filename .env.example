# Kiwoom API Configuration
KIWOOM_ACCOUNT=
KIWOOM_PASSWORD=

# Database Configuration (TimescaleDB on Mac)
TIMESCALE_HOST=192.168.x.x  # Mac M3 Max IP address
TIMESCALE_PORT=5432
TIMESCALE_DB=kiwoom_data
TIMESCALE_USER=postgres
TIMESCALE_PASSWORD=

# Vector Database (Milvus on Mac)
MILVUS_HOST=192.168.x.x
MILVUS_PORT=19530

# SFTP Configuration (Windows â†’ Mac sync)
SFTP_HOST=192.168.x.x
SFTP_PORT=22
SFTP_USER=
SFTP_PASSWORD=
SFTP_REMOTE_PATH=/path/to/data

# Data Collection Settings
RATE_LIMIT_REQUESTS_PER_SECOND=4  # Safe margin (Kiwoom limit is 5)
BUFFER_DB_PATH=./data/buffer.db   # SQLite buffer
EXPORT_INTERVAL_SECONDS=3600      # Export to Parquet every 1 hour

# Logging
LOG_LEVEL=INFO
LOG_PATH=./logs
